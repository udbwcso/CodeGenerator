        <result column="label_name" property="labelName" jdbcType="VARCHAR"/>
        <result column="score" property="score" jdbcType="INTEGER"/>
    </resultMap>

    <resultMap id="ProductMapper" type="com.ymt.wxshop.product.domain.Product">
        <id column="goods_info_id" property="id" jdbcType="BIGINT"/>
        <result column="goods_info_name" property="title" jdbcType="VARCHAR"/>
        <result column="goods_info_item_no" property="no" jdbcType="VARCHAR"/>
        <result column="goods_info_deliveryNo" property="deliveryNo" jdbcType="VARCHAR"/>
        <result column="goods_info_added" property="added" jdbcType="CHAR"/>
        <result column="goods_info_create_time" property="createTime" jdbcType="TIMESTAMP"/>
        <result column="goods_info_prefer_price" property="preferPrice" jdbcType="DECIMAL"/>
        <result column="goods_info_market_price" property="marketPrice" jdbcType="DECIMAL"/>
        <result column="goods_info_settlePrice" property="settlePrice" jdbcType="DECIMAL"/>
        <result column="goods_info_stock" property="stock" jdbcType="BIGINT"/>
        <result column="goods_info_img_id" property="listImage" jdbcType="VARCHAR"/>
        <result column="goods_info_brokerage" property="brokerage" jdbcType="DECIMAL"/>
        <result column="goods_info_type" property="type" jdbcType="CHAR"/>
        <result column="goods_info_fristProfit" property="fristProfit" jdbcType="BIGINT"/>
        <result column="goods_info_secondProfit" property="secondProfit" jdbcType="BIGINT"/>
