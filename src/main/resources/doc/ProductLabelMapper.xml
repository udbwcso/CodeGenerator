        <result column="is_redemption" property="redemption" jdbcType="VARCHAR"/>
        <result column="sale_integral" property="saleIntegral" jdbcType="DECIMAL"/>
        <result column="integral_money" property="integralMoney" jdbcType="DECIMAL"/>
        <result column="score" property="score" jdbcType="BIGINT"/>
        <result column="image_in_name" property="appImg" jdbcType="VARCHAR"/>
        <result column="third_id" property="thirdId" jdbcType="BIGINT"/>
        <result column="goods_info_pv" property="productPv" jdbcType="DECIMAL"/>
        <result column="home_page_show" property="homePageShow" jdbcType="VARCHAR"/>
        <result column="show_score" property="showScore" jdbcType="BIGINT"/>
    </resultMap>

</mapper>