<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ymt.wxshop.product.dao.ProductLabelMapper">
    <resultMap id="ProductLabel" type="com.ymt.wxshop.product.domain.ProductLabel">
